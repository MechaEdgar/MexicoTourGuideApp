<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.mexicocitytourguideapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MuseumActivity"
            android:label="MuseumsActivity" />
        <activity
            android:name=".MonumentsActivity"
            android:label="MonumentsActivity" />
        <activity
            android:name=".TacosActivity"
            android:label="TacosActivity" />
        <activity android:name=".BakeriesActivity"
            android:label="BakeriesActivity"></activity>
    </application>

</manifest>